Publisher: Educational Publishing Foundation},
	keywords = {Body Image, Measurement, Muscle Tone, Objectification, Self-Concept, Self-Evaluation, Test Construction, Test Validity},
	file = {Snapshot:/Users/jessicapardimaraujo/Zotero/storage/6PITH82S/2013-05711-001.html:text/html},
