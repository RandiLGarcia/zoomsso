	keywords = {Academic performance decrement, Fear of missing out ({FoMO}), Online privacy, Self-disclosure, Social comparison, Social media fatigue},
	file = {Full Text PDF:/Users/jessicapardimaraujo/Zotero/storage/IIVQUNEJ/Malik et al. - 2020 - Correlates of social media fatigue and academic pe.pdf:application/pdf},
