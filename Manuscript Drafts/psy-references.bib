@article{gay_my_2010,
	title = {My body or my mind: The impact of state and trait objectification on women's cognitive resources},
	volume = {40},
	issn = {1099-0992},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/ejsp.731},
