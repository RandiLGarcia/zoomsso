Publisher: Educational Publishing Foundation},
	keywords = {Emotional States, Life Satisfaction, Negative Emotions, Objectification, Online Social Networks, Self-Perception, Social Media, Well Being},
	file = {Snapshot:/Users/jessicapardimaraujo/Zotero/storage/9BEEDFAD/2021-79562-001.html:text/html},
